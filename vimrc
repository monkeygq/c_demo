set nocompatible
filetype off

set rtp+=~/.vim/bundle/Vundle.vim
call vundle#begin()

Plugin 'VundleVim/Vundle.vim'
Plugin 'ervandew/supertab'

call vundle#end()
filetype plugin indent on
set number
set cursorline
set ruler
set hlsearch
set tabstop=4
set softtabstop=4
set shiftwidth=4
set noexpandtab
set laststatus=2

set smartindent
set cindent
